    <meta name="author" content="crenoveative">
    <meta name="viewport" content="width=device-width, initial-scale=1, maximum-scale=1">

    <!-- Fav and Touch Icons -->
    <link rel="apple-touch-icon-precomposed" sizes="144x144" href="images/ico/apple-touch-icon-144-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="114x114" href="images/ico/apple-touch-icon-114-precomposed.png">
    <link rel="apple-touch-icon-precomposed" sizes="72x72" href="images/ico/apple-touch-icon-72-precomposed.png">
    <link rel="apple-touch-icon-precomposed" href="images/ico/apple-touch-icon-57-precomposed.png">
    <link rel="shortcut icon" href="images/favicon.ico">

    <!-- Font face -->
    <link href="https://fonts.googleapis.com/css?family=Playfair+Display:400,400i,700,700i|Source+Sans+Pro:200,200i,300,300i,400,400i,600,600i,700,700i,900,900i&display=swap" rel="stylesheet">
    <link href="/sites/all/themes/aviacollect/font-faces/metropolis/metropolis.css" rel="stylesheet">

    <!-- CSS Plugins -->
    <link href="/sites/all/themes/aviacollect/css/font-icons.css" rel="stylesheet">
    <link rel="stylesheet" type="text/css" href="/sites/all/themes/aviacollect/bootstrap/css/bootstrap.min.css" media="screen">
    <link href="/sites/all/themes/aviacollect/css/animate-min.css" rel="stylesheet">
    <link href="/sites/all/themes/aviacollect/css/main.css" rel="stylesheet">
    <link href="/sites/all/themes/aviacollect/css/plugin.css" rel="stylesheet">

    <!-- CSS Custom -->
    <link href="/sites/all/themes/aviacollect/css/style.css" rel="stylesheet">

    <!-- CSS Custom -->
    <link href="/sites/all/themes/aviacollect/css/your-style.css" rel="stylesheet">

    <!-- HTML5 shim and Respond.js for IE8 support of HTML5 elements and media queries -->
    <!--[if lt IE 9]>
		<script src="https://oss.maxcdn.com/html5shiv/3.7.2/html5shiv.min.js"></script>
		<script src="https://oss.maxcdn.com/respond/1.4.2/respond.min.js"></script>
	<![endif]-->

</head>
<body class="with-waypoint-sticky">
    {#
      Keyboard navigation/accessibility link to main content section in
      page.html.twig.
    #}
    
    
    {{ page }}

    
    <!-- JS -->
                <script type="text/javascript" src="/sites/all/themes/aviacollect/js/jquery-2.2.4.min.js"></script>
                <script type="text/javascript" src="/sites/all/themes/aviacollect/js/plugins.js"></script>
                <script type="text/javascript" src="/sites/all/themes/aviacollect/js/custom-core.js"></script>
